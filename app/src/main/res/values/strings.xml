    <!-- database -->
    <string-array name="db_table_name">
        <item>ADDR</item>
        <item>TXS</item>
        <item>CURRENT</item>
        <item>LOGIN</item>

    </string-array>

    <!--table-->
    <string-array name="ADDR">
        <item>WID               TEXT  NOT NULL   </item>
        <!--WALLET ID(CARD ID)-->
        <item>ACCOUNT_ID        INTEGER  NOT NULL   </item>
        <item>ADDRESS           TEXT  NOT NULL      </item>
        <item>KCID              INTEGER  NOT NULL   </item>
        <!--0:External ; 1:Internal-->
        <item>KID               INTEGER   NOT NULL  </item>
        <!--Address KID-->
        <item>N_TX              INTEGER   NOT NULL  </item>
        <item>ADDRESS_BALANCE   BIGINT   NOT NULL   </item>
        <item>ADDRESS_LABEL     TEXT                </item>
        <item>PRIMARY KEY (WID,ADDRESS) </item>
    </string-array>

    <string-array name="TXS">
        <item>WID               TEXT  NOT NULL   </item>
        <item>ACCOUNT_ID        INTEGER  NOT NULL   </item>
        <item>ADDRESS           TEXT  NOT NULL      </item>
        <item>TX_ID             TEXT     NOT NULL   </item>
        <item>TX_ADDRESS        TEXT  NOT NULL      </item>
        <item>TX_RESULT         BIGINT  NOT NULL   </item>
        <item>TX_DATE           TEXT     NOT NULL   </item>
        <item>TX_CONFIRMATION   INTEGER  			</item>
        <item>ID   INTEGER PRIMARY KEY</item>
    </string-array>

    <string-array name="CURRENT">
        <item>WID               TEXT  NOT NULL   </item>
        <item>COUNTRY           TEXT    NOT NULL   </item>
        <!--WALLET ID(CARD ID)-->
        <item>RATES             REAL    NOT NULL   </item>
        <item>PRIMARY KEY (WID,COUNTRY) </item>
    </string-array>

    <string-array name="LOGIN">
        <item>DATE              TEXT    NOT NULL   </item>
        <item>WID               TEXT    NOT NULL   </item>
        <!--WALLET ID(CARD ID)-->
        <item>UUID              TEXT    NOT NULL   </item>
        <item>OTP               TEXT    NOT NULL   </item>
        <item>PRIMARY KEY (WID) </item>
    </string-array>
    <!--jSon parser-->
    <string-array name="addressInfo">
        <item>address</item>
        <item>is_unknown</item>
        <item>balance</item>
        <item>balance_multisig</item>
        <item>totalreceived</item>
        <item>nb_txs</item>
    </string-array>

